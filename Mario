#include <cs50.h>
#include <studio.h>

viod draw (int h);

int main (void)
{
   int height=get_int("Height: ");
   draw (height);
 }
void draw (int h)
{
   for(int i=1;i<=h;i++)
   {
      for(int j=h;j>i;j--)
      {
         printf(" ");
       }
       for(j;j>0;j--)
       {
         printf("#");
        }
     printf("\n");
     }
  }

